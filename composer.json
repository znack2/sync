{
  "name": "freshplan/sync",
  "description": "A sync engine integration",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5.0",
    "graham-campbell/testbench": "^4.0",
    "nunomaduro/collision": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Usedesk\\SyncIntegration\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Usedesk\\SyncIntegration\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Usedesk\\SyncIntegration\\ServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
