{
    "name": "usedesk/sync-engine-integration",
    "description": "sync-engine-integration",
    "license": "MIT",
    "keywords": [
        "sync-engine",
        "integration"
    ],
    "authors": [
        {
            "name": "Injir",
            "email": "sourinjir@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.5.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Usedesk\\SyncEngineIntegration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Usedesk\\SyncEngineIntegration\\Tests\\": "tests"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    },
    "scripts": {
        "phpunit": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Usedesk\\SyncEngineIntegration\\SyncEngineIntegrationServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}